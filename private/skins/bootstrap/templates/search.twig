{% extends "_layout.twig" %}

{% block title %}Search Results for %s", [query]) }}{% endblock %}

	{% block content %}
        <h3>Search Results for %s", [query]) }}</h3>
        {% for video in videos %}
            {{ video_box(video) }}
        {% else %}
            <p>No search results found.</p>
        {% endfor %}
    {% endblock %}
