<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %} - {{ website_branding.name }}</title>
    {% block head %}{% endblock %}
    <link rel="manifest" href="/assets/icons/site.webmanifest">
    <link rel="shortcut icon" href="/assets/icons/favicon.ico">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/icons/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/assets/icons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/assets/icons/favicon-16x16.png">
    <link rel="mask-icon" href="/assets/icons/safari-pinned-tab.svg" color="#1b93ff">
    <meta name="apple-mobile-web-app-title" content="{{ website_branding.name }}">
    <meta name="application-name" content="{{ website_branding.name }}">
    <meta name="msapplication-TileColor" content="#1d3fc4">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="/https://cdn.jsdelivr.net/npm/@tabler/icons@latest/iconfont/tabler-icons.min.css">
    <link rel="stylesheet" href="/assets/css/finalium.css" type="text/css">
    {% if current_theme == 'dark' %}
        <link id="darkthm" rel="stylesheet" href="/assets/css/finalium_darkmode.css" type="text/css">
    {% elseif current_theme == 'nostalgia' %}
        <link id="nostalgia" rel="stylesheet" href="/assets/css/finalium_nostalgia.css" type="text/css">
    {% elseif current_theme == 'qobo' %}
        <link id="nostalgia" rel="stylesheet" href="/assets/css/finalium_qobo.css" type="text/css">
    {% endif %}
    <script src="https://cdn.jsdelivr.net/npm/js-cookie@rc/dist/js.cookie.min.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.1.js" integrity="sha256-3zlB5s2uwoUzrXK3BT7AX3FyvojsraNFxCc2vC/7pNI=" crossorigin="anonymous"></script>
    <script src="/assets/common.js"></script>
    <script>
        subscribe_string = 'Follow';
        unsubscribe_string = 'Unfollow';
    </script>
</head>
<body class="chaziz-body">
{% block precontent %}{% endblock %}
<div class="container page-container chaziz-container">
    {% block content %}{% endblock %}
</div>
</div>
{% block custom_footer %}
{% endblock %}
</body>
</html>