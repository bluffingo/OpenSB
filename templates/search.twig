{% extends "_layout.twig" %}

{% block title %}{{ __("Search Results for %s", [query]) }}{% endblock %}
	{% block content %}
		<h3>{{ __("Search Results for %s", [query]) }}</h3>
		{% for video in videos %}
			{{ video_box(video) }}
		{% else %}
			<p>{{ __("No search results found.") }}</p>
		{% endfor %}
	{% endblock %}
